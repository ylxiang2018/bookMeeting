{
  "name": "project_template_react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:client": "vite --host --port 3000",
    "dev:client:3002": "vite --host --port 3002",
    "dev:client:both": "concurrently \"npm run dev:client\" \"npm run dev:client:3002\"",
    "dev": "npm run dev:client",
    "dev2": "npm run dev:client:3002",
    "build:client": "vite build --outDir dist/static",
    "build": "if exist dist rd /s /q dist && npm run build:client && copy package.json dist && type nul > dist/build.flag",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
    "commit": "git-cz"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^12.9.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.3.0",
    "recharts": "^2.15.1",
    "sonner": "^2.0.2",
    "tailwind-merge": "^3.0.2",
    "uuid": "^9.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsonc-eslint-parser": "^2.4.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}